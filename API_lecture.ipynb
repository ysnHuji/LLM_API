    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "\n",
    "def scrape_page(url):\n",
    "    response = requests.get(url)\n",
    "    if response.status_code == 200:\n",
    "        soup = BeautifulSoup(response.content, 'html.parser')\n",
    "        reviews = soup.find_all('div', class_='description')\n",
    "        page_reviews = []\n",
    "\n",
    "        for review in reviews:\n",
    "            review_text_element = review.find(class_='description-text')\n",
    "            if review_text_element and review_text_element.get_text(strip=True):\n",
    "                review_text = review_text_element.get_text(strip=True)\n",
    "                page_reviews.append({'review': review_text})\n",
    "            else:\n",
    "                # Skip empty reviews or keep the placeholder text as per your requirement\n",
    "                continue  # Skip this review if it's empty\n",
    "\n",
    "        return page_reviews\n",
    "    else:\n",
    "        return []\n",
    "\n",
    "# Base URL without the page number\n",
    "base_url = \"https://reviews.webmd.com/drugs/drugreview-3441-methotrexate-anti-rheumatic-oral?conditionid=&sortval=1&page=\"\n",
    "page = 1\n",
    "all_reviews = []\n",
    "\n",
    "while True:\n",
    "    print(f\"Scraping page {page}...\")\n",
    "    current_url = base_url + str(page) + \"&next_page=true\"\n",
    "    reviews = scrape_page(current_url)\n",
    "\n",
    "    # Check if the current page returned reviews\n",
    "    if reviews:\n",
    "        all_reviews.extend(reviews)\n",
    "        page += 1\n",
    "    else:\n",
    "        # If no reviews are returned, it might be the end or an issue with the page, so break\n",
    "        break\n",
    "\n",
    "df_reviews = pd.DataFrame(all_reviews)\n"
